<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>About - Travlr Getaways Web Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{>header}}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							{{#each about}}
							{{{this.division}}}
							<h2>{{this.title}}</h2>
							{{{this.article}}}
							{{/each}}
						</div>
					</div>
				</div>
			</div>
		</div>
		{{>footer}}
	</div>
</body>
</html>